file tux_small.png # 查看文件的类型
more <filename> # 仅仅看一页内容
grep ssh <filename>
tr " " "\n" # 把空格替换成换行
sort    # 根据ascii码对行进行排序
    -r  # 逆序
wc  # 数单词个数
wc < RightNow.txt
test    # 测试文件是否存在等等 存在返回0 否则返回1
true && echo "Yes." # 第一个成功了，第二个才执行
false || echo "Yes."    # 第一个失败了，第二个才执行
command1 && command2 && command3 && command4
